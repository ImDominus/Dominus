import numpy as np
import matplotlib.pyplot as plt
import pandas as pd


dataset = pd.read_csv('Data.csv')
X = dataset.iloc[:, :-1].values
Y = dataset.iloc[:, 3].values
## iloc format -> [range(x), range(y)]
## like this
'''
 1  3   4
 3  5   1
'''
## range(x) is think (1, 3, 4), (3, 5, 1)
## range(y) is think (1, 3), (3, 5), (4, 1)
## this is equal to python list fuction
